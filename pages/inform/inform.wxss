.content{
  text-align: center;
  padding:40rpx 0;
  background: #d24d46;
}
.content .avatarUrl image{
    width:140rpx;
    border-radius: 50%;
    border:2px solid #fff;
    box-sizing: border-box;
}
.avatarUrl .nickName{
  color:#fff;
}
.content .question{
  font-size: 40rpx;
  color: #f3dab2;
  padding:20rpx 0;
}
.content .question image{
    width: 60rpx;
    border:1px solid #ddd;
}
.content .type{
    width:80%;
    margin: 10rpx auto;
    border-width:1px;
    font-size:28rpx;
    border-radius:4px;
    text-align:center;
    line-height:80rpx;
}
.content .type.no{
    background: #dc7264;
    color: #fff;
}
.content .type.yes{
    background: #f3dab2;
    color: #d24d46;
}
.content .money{
  text-align: center;
  font-size: 26rpx;
  color: #f3dab2;
}
.content .money text{
  font-size: 36rpx;
}
.content .tabs{
  display: flex;
  justify-content: space-around;
  color: #fff;
  font-size: 26rpx;
  vertical-align: middle;
  margin: 30rpx 0;
}
.content .tabs .item{
  padding:20rpx 0;
  cursor: pointer;
}
.content .tabs .item image{
   width: 30rpx;
   margin-right: 10rpx;
   position: relative;
   top:5rpx;
}
.all{
  font-size: 26rpx;
  background: #f5f5f5;
}
.all .notice{
  width: 100%;
  color:#626262;
  padding:20rpx;
  box-sizing: border-box;
  
}
.list{
  display: -webkit-box;
  display: -webkit-flex;
  display: flex;
  -webkit-box-align: center;
  -webkit-align-items: center;
  align-items: center;
  flex-direction: row;
  padding:20rpx;
  margin: 20rpx 0;
  background: #fff;
}
.list .top{
  margin-right: 10rpx;
  width: 100rpx;
  height: 100rpx;
  text-align: center;
}
.keepList{
  webkit-box-flex: 1;
  -webkit-flex: 1;
  flex: 1;
  min-width: 0;
}
.list .top image{
  width: 80rpx;
  border-radius: 4rpx;
  border:1px solid #fff;
}
.keepList .itemkeep{
  background: #fff;
  border-radius: 10rpx;
  box-sizing: border-box;
  color: #626262;
}
.keepList .itemkeep .item .left{
  text-align: left;
  display: inline-block;
  width: 69%;
  white-space:nowrap; overflow:hidden; text-overflow:ellipsis;
}
.keepList .itemkeep .item .right{
  display: inline-block;
  width: 30%;
  text-align: right;
}
.time .left{
  text-align: left;
}
.keepList .itemkeep .sum .right,.time .left{
  color: #d24d46;
}
.itemkeep .sum .left image{
  width:20rpx;
}
.Tword,.false,.success{
  background: #fff;
  border-radius: 10rpx;
  width:90%;
  position: fixed;
  z-index: 999;
  top: 50%;
  left: 50%;
  -webkit-transform: translate(-50%,-50%);
  transform: translate(-50%,-50%);
  background-color: #fff;
  text-align: center;
  overflow: hidden;
  padding: 20rpx 0;
}
.Tword,.time{
  text-align: center;
}
.Tword .tishi,.false .tishi{
  font-weight: bold;
  font-size: 32rpx;
  margin: 20rpx 0;
}
.time .now{
  display: inline-block;
  border:2px solid #d24d46;
  width: 80rpx;
  height: 80rpx;
  line-height: 80rpx;
  border-radius: 50%;
  margin: 20rpx 0;
}
.Tword .red{
  color: #d24d46;
  margin: 20rpx 0;
}
.grid{
  padding: 30rpx 0;
}
.grid .grid-item{
  display: inline-block;
  background:#fff;
  color: #d24d46;
  border-radius: 5rpx;
  margin: 10rpx;
  padding: 10rpx 20rpx;
  border: 1px solid #d24d46;
}
.grid .grid-item.item{
  background:#d24d46;
  color: #fff;
}
.false .img image{
    width: 80rpx;
}
.false .button{
  display: flex;
  justify-content: space-around;
  font-size: 28rpx;
}
.false .button view{
  margin:10rpx auto;
  border-width:1px;
  border-radius:4px;
  text-align:center;
  line-height:80rpx;
  border:1px solid #ddd;
  width: 180rpx;
}
view.close{
  background: #fff;
  color: #000;
}
view.again{
  background:#d24d46;
  color: #fff;
}
.success{
  background:#fff url(http://ovhvevt35.bkt.clouddn.com/bao/redhalf.png) no-repeat 100% 20%;
  background-size: contain;
  border:3rpx solid #f3dab2;
}
.success .avatarUrl image{
    width:140rpx;
    border-radius: 50%;
    border:2px solid #fff;
    box-sizing: border-box;
}
.content .question{
  position: relative;
}
.question .big{
  width:60rpx;
  height: 60rpx;
  position: absolute;
  top: 40%;
  left: 50%;
  transform: scale(1) translate(-50%, -50%);
  background: #000;
  opacity: 0.5;
}
.question .big image{
  width:30rpx;
  border:none;
}
.avatarUrl .nickName{
  color:#fff;
}
.success .question{
  color:#f3dab2;
}
.question image{
    width: 60rpx;
    border:1px solid #ddd;
}
.open image{
  width: 160rpx;
}
/*背景  */
.bg{
  position: absolute;
  top: 0;
  left: 0;
  width: 100%;
  height: 100%;
  background: #000;
  opacity: 0.7;
  z-index: 9;
}
.shengyi{
  display: inline-block;
  position: relative;
  top:10rpx;
}
.shengyi image{
    width: 50rpx;
    position: relative;
    top:6rpx;
    margin-left: 30rpx;
    cursor: pointer;
}
.pintu{
  font-size: 26rpx;
  text-align: center;
  position: absolute;
  top: 0;
  left: 0;
  width: 100%;
  z-index: 999;
}
.pintu .tishi{
  text-align: center;
  color: #fff;
  padding: 30rpx 0;
}
audio{
  position: absolute;
  top: 0;
  right: 0;
  opacity: 0;
  width: 50rpx;
  height: 50rpx;
}
.pinimg{
  width:80%;
  margin: 30rpx auto;
}
.pinimg image{
  width:33%;
  border:1px solid #ddd;
  box-sizing: border-box;
}