.cameraphone {
  border:none!important;
}
.result .little{
    font-size: 24rpx;
}
.left .bga{
  background: #d24d46;
  color: #fff;
  font-size: 22rpx;
  padding: 0 10rpx;
}
.footer{
  width:100%;
  position: fixed;
  bottom: 0;
  left: 0;
  text-align: center;
  font-size: 32rpx;
  background: #f2f2f2;
  z-index: 9;
}
.footer .set{
  background: #fff;
  padding: 20rpx 0;
}
.footer .set1{
  margin-bottom: 10rpx;
