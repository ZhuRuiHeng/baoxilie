/* pages/demo/demo.wxss */
audio {
  display: inline-block;
  line-height: 0;
}
audio > .audio-default {
  max-width: 100%;
  min-width: 302px;
  height: 65px;
  background: #fcfcfc;
  border: 1px solid #e0e0e0;
  border-radius: 2.5px;
  display: inline-block;
  overflow: hidden;
}
audio  .audio-default  .audio-left {
  width: 65px;
  height: 65px;
  float: left;
  background-color: #e6e6e6;
  background-size: 100% 100%;
  background-position: 50% 50%;
}
audio  .audio-default  .audio-left  .audio-button {
  width: 24px;
  height: 24px;
  margin: 20.5px;
  background-size: cover;
}
audio  .audio-default  .audio-left  .audio-button.play {
  background-image: url('data:image/png;base64,');
}
audio  .audio-default  .audio-right {
  box-sizing: border-box;
  height: 65px;
  margin-left: 65px;
  padding: 11px 16.5px 13.5px 15px;
  overflow: hidden;
}
audio  .audio-default  .audio-right  .audio-info {
  margin-right: 70px;
  overflow: hidden;
}
audio  .audio-default  .audio-right  .audio-info  .audio-name {
  height: 22.5px;
  line-height: 22.5px;
  margin-bottom: 3.5px;
  font-size: 14px;
  color: #353535;
  overflow: hidden;
  white-space: nowrap;
  text-overflow: ellipsis;
}
audio .audio-default .audio-right  .audio-info .audio-author {
  height: 14.5px;
  line-height: 14.5px;
  font-size: 12px;
  color: #888888;
  overflow: hidden;
  white-space: nowrap;
  text-overflow: ellipsis;
}
audio .audio-default .audio-right .audio-time {
  margin-top: 3.5px;
  height: 16.5px;
  font-size: 12px;
  color: #888888;
  float: right;
}
@font-face {
  font-weight: normal;
  font-style: normal;
  font-family: "weui";
  src: url('data:application/octet-stream;base64,') format('truetype');
}

view {
  display: block;
}
navigator {
  height: auto;
  width: auto;
  display: block;
}
